{
  "name": "franca-languageserver",
  "displayName": "Franca language server",
  "description": "A Franca language server extension for VS Code",
  "version": "0.0.1",
  "publisher": "wmanth",
  "icon": "images/itemis.png",
  "license": "EPL-1.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/wmanth/franca-languageserver.git"
  },
  "bugs": {
    "url": "https://github.com/wmanth/franca-languageserver/issues"
  },
  "engines": {
    "vscode": "^1.33.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "activationEvents": [
    "onLanguage:franca"
  ],
  "main": "out/extension",
  "contributes": {
    "languages": [
      {
        "id": "franca",
        "aliases": [
          "franca"
        ],
        "extensions": [
          ".fidl",
          ".fdepl"
        ],
        "configuration": "./franca.configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "franca",
        "scopeName": "text.franca",
        "path": "./syntaxes/franca.tmLanguage.json"
      }
    ],
    "commands": [
      {
        "command": "mydsl.a.proxy",
        "title": "MyDsl Command A"
      },
      {
        "command": "mydsl.b",
        "title": "MyDsl Command B"
      }
    ]
  },
  "devDependencies": {
    "vscode": "^1.1.33",
    "typescript": "^3.3.400",
    "@types/node": "^11.13.2"
  },
  "dependencies": {
    "vscode-languageclient": "^5.2.1"
  },
  "scripts": {
    "prepublish": "node ./node_modules/vscode/bin/install && tsc -p ./src",
    "compile": "tsc -p ./src",
    "watch": "tsc -w -p ./src",
    "update-vscode": "node ./node_modules/vscode/bin/install"
  }
}
